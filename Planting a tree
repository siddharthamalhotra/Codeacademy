// Define your tree class in this file!
class Tree {
  public static void main(String[] args)
  {
    // introduction
    System.out.println("Hey there, I'm Sid and I'm practicing Java");

    //tree action
    System.out.println("I'm going to plant a tree today");

    // tree printing
    System.out.println("  *  ");
    System.out.println(" *** ");
    System.out.println(" *** ");
    System.out.println("  *  ");
    System.out.println("  *  ");   

  }
}
